{
  "compilerOptions": {
     "jsx": "react-jsx",  
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "target": "es6",         
    "lib": ["es2015", "dom"], 
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}

